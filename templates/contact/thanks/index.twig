{% extends "_layouts/site.twig" %}

{% block content %}

    {% macro errorList(errors) %}
        {% if errors %}
            <ul class="errors">
                {% for error in errors %}
                    <li>{{ error }}</li>
                {% endfor %}
            </ul>
        {% endif %}
    {% endmacro %}

    {% from _self import errorList %}


    <section>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="page-title">
                        <div class="row">
                            <div class="col-lg-9">
                                <h1><span>Thank's</span> for your message.</h1>

                                <h2 class="">I will be in touch soon.</h2>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="margin-t-30">
        <div class="container">

            <div class="row margin-t-50">
                <div class="col-lg-4 address-box">
                    <div>
                        <h5>Email Address</h5>
                        <p class="text-muted">hello@ashleyluff.com</p>
                    </div>
                    <div class="margin-t-30">
                        <h5>Telephone</h5>
                        <p class="text-muted">+447808987213</p>
                    </div>
                    <div class="margin-t-30">
                        <h5>Address</h5>
                        <p class="text-muted">THe studio, Brighton</p>
                    </div>
                </div>
                <div class="col-lg-8">


                </div>
            </div>


        </div> <!-- end container -->
    </section>









{% endblock %}